{
  "name": "phasty",
  "description": "Shopping cart on phalcon php framework.",
  "keywords": [
    "shopping cart",
    "phalcon"
  ],
  "require": {
    "brandonwamboldt/utilphp": "~1.0",
    "phalcon/incubator": "v3.1.1",
    "imagecow/imagecow": "1.*",
    "philipbrown/basket": "dev-master",
    "firebase/php-jwt": "^2.1",
    "phalcon-ext/mailer": "^2.0",
    "moneyphp/money": "^3.0",
    "omnipay/omnipay": "~2.3.2",
    "clippings/omnipay-paypal": "^0.2.0",
    "braintree/braintree_php": "^3.23"
  },
  "require-dev": {
    "fzaninotto/faker": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Phasty\\": "app",
      "Phalcon\\": "Phalcon"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "stable"
}
