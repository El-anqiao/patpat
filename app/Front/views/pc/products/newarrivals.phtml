        function reloadData() {
            var filters = {};
            var last_id = 0;
            var asides = $('div aside.widget-sidebar');
            $.each(asides, function (i, value) {
                if ($(this).find('h2').is('h2')) {
                    var key = $(this).find('h2').html();
                    var values = $(this).find('.flchecked').map(function () {
                        return $(this).parent().text();
                    }).toArray();
                    filters[key] = values;
                }
            });
            $("#reload_form").submit();
        }

        $(".flcheckbox").on("click",function(){
            var checkboxClass = $(this).attr("class");
            if (!$(this).hasClass("flchecked")) {
                $(this).addClass("flchecked");
            } else {
                $(this).removeClass("flchecked");
            }
            setTimeout(function(){
                $("input[name='last_id']").val(0);
                $("input[name='page']").val(1);
                reloadData();
            },500);
        });

        //select sort change
        $('select.form-control').on('change', function () {
            $("input[name='page']").val(1);
            sortSelected = $(this).children(':selected').val();
            $("input[name='sort']").val(sortSelected);
            reloadData();
        });

        $(document).on("click",".go_page",function(){
            var page_count = parseInt("80");
            $("input[name='sort']").val(sortSelected);
            var page = parseInt($(this).data("id"))-1;
            var last_id = page*page_count;
            $("input[name='last_id']").val(last_id);
            $("input[name='page']").val(page+1);
            reloadData();
        });
    });




    /*new arrivals*/
    var productNumber = $(".category-list-item");
    var productHeight = productNumber.eq(0).outerHeight()/3;
    var scrollTopArray = [],productHeightArray = [];
    productNumber.each(function(){
        productHeightArray.push($(this).offset().top +productHeight);
    });
    $(window).resize(function(){
        productHeight = productNumber.eq(0).outerHeight()/3;
        productHeightArray = [];
        productNumber.each(function(){
            var _this = $(this);
            productHeightArray.push(_this.offset().top +productHeight);
        });
        scrollTopArray = [];
    });
    var clientH = $(window).height(),maxHeight = clientH,currentIndex,maxIndexNumber;
    if(productHeightArray[0]>maxHeight){
        currentIndex = -1;
    }
    productHeightArray.forEach(function(e,index){
        if(e >= maxHeight){
            return false;
        }
        currentIndex = index;
    });
    maxIndexNumber = currentIndex + 1;

    /* maxIndexNumber*/
    $(window).scroll( function() {
        var windowTop=$(document).scrollTop();
        scrollTopArray.push(windowTop);
        maxHeight = clientH + Math.max.apply(null, scrollTopArray);
        productHeightArray.forEach(function(e,index){
            if(e >= maxHeight){
                return false;
            }
            currentIndex = index;
        });
        maxIndexNumber = currentIndex + 1;
    });

    /**/
    var categoryArray = [];

    //  
    var _href = "/new-arrivals-data?category_id=&page=1";
    var page_index_all = 2,isloaded_all = false;
    var page_index_18 = 1,isloaded_18 = false;
    var page_index_19 = 1,isloaded_19 = false;
    var page_index_20 = 1,isloaded_20 = false;
    var page_index_21 = 1,isloaded_21 = false;
    var page_index_22 = 1,isloaded_22 = false;

    var the = true;
    var c_id = '';
    var dovBox = $("#category_all");
    var categoryIndexArr = [];
    var categoryIndex = $(".new_arrivals_btn1 li").index($('.current'));
    categoryIndexArr[0] = categoryIndex;

    //  id
    var arrBtn1 = $(".arr-btn").eq(0).find("a").data("id"),
        arrBtn2 = $(".arr-btn").eq(1).find("a").data("id"),
        arrBtn3 = $(".arr-btn").eq(2).find("a").data("id"),
        arrBtn4 = $(".arr-btn").eq(3).find("a").data("id");


    $(".new_arrivals_btn1 li").click(function(){
        if(!categoryArray[(categoryIndexArr[0])] || maxIndexNumber>categoryArray[(categoryIndexArr[0])]){
            categoryArray[(categoryIndexArr[0])] = maxIndexNumber;               /**/
        }
        var _thisId = $(this).find("a").data("id");
        var index = $(this).index();
        categoryIndexArr[0] = index;
        categoryIndex = index;

        $(this).addClass("current").siblings().removeClass("current");
        $(".category_listview_baby").hide().eq(index).show();
        c_id = _thisId;
        if( _thisId == arrBtn1){
            if( page_index_19  == 1 ){
                $(".loading_img").show();
                loadingDome(_thisId, page_index_19);
            }
            dovBox = $("#category_"+arrBtn1);
        }else if( _thisId == arrBtn2){
            if( page_index_20  == 1 ){
                $(".loading_img").show();
                loadingDome(_thisId, page_index_20);
            }
            dovBox = $("#category_"+arrBtn2);
        }else if( _thisId == arrBtn3){
            if( page_index_21 == 1 ){
                $(".loading_img").show();
                loadingDome(_thisId, page_index_21);
            }
            dovBox = $("#category_"+arrBtn3);
