        var newText = $(".new_arrivals_btn1 .current").text();
        sessionStorage.setItem("arrivalsNames", newText);
    });
    $(".new_arrivals_btn1 li").each(function(){
        var arrivalsPageName = $(this).data("name");
        var arrivalsNavName = $.trim(sessionStorage.getItem("arrivalsNames"));
        if( arrivalsPageName == arrivalsNavName ){
            $(this).trigger("click");
            sessionStorage.removeItem("arrivalsNames");
        }
    });
    $(window).scroll(function(){
        var Tops = $(window).scrollTop();

        var _scrolHeight;
        if( $(".country-show-selector").text() == "Singapore" || $(".country-show-selector").text() == "Malaysia" ){
            var _scrolHeight = $("#site-footer-services").offset().top - $(window).height();
        }else{
            var _scrolHeight = $("#site-footer").offset().top - $(window).height();
