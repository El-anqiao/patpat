                loadingDome(c_id,page_index);
                the = false;
            }
        }
    });

    function loadingDome(category_id, page){
        var url = "/new-arrivals-data?category_id="+category_id +"&page="+page;
        $.ajax({
            type:"get",
            url:url,
            dataType:"json",
            success:function(data){
                the = true;
                $(".loading_img").hide();
                var datalength = Object.keys(data).length;
                if( category_id == arrBtn1){
                    if( datalength == 0 ){
                        isloaded_19 = true;
                        dovBox.next(".loading_img").addClass("loading_img-no");
                        return false;
                    }
                    page_index_19 ++;
                }else if( category_id == arrBtn2){
                    if( datalength == 0 ){
                        isloaded_20 = true;
                        dovBox.next(".loading_img").addClass("loading_img-no");
                        return false;
                    }
                    page_index_20 ++;
                }else if( category_id == arrBtn3){
                    if( datalength == 0 ){
                        isloaded_21 = true;
                        dovBox.next(".loading_img").addClass("loading_img-no");
                        return false;
                    }
                    page_index_21 ++;
                }else if( category_id == arrBtn4){
                    if( datalength == 0 ){
                        isloaded_22 = true;
                        dovBox.next(".loading_img").addClass("loading_img-no");
                        return false;
                    }
                    page_index_22 ++;
                }else if( category_id == "18"){
                    if( datalength == 0 ){
                        isloaded_18 = true;
                        dovBox.next(".loading_img").addClass("loading_img-no");
                        return false;
                    }
                    page_index_18 ++;
                }else if( category_id == ""){
                    if( datalength == 0 ){
                        isloaded_all = true;
                        dovBox.next(".loading_img").addClass("loading_img-no");
                        return false;
                    }
                    page_index_all ++
                }
                for( dataList in data ){
                    $(".loading_img").hide();
                    $(".placeholder").hide();
                    var msrp = data[dataList]["msrp"];
                    var event_price = data[dataList]["event_price"];
                    var arrHtml = '<div class="col-xs-6 col-sm-4 col-md-3 col-lg-5ths category-list-item show_item empty_list" data-id="'+ data[dataList]["product_id"] +'">';
                    arrHtml += '<article class="box-product alt"><a href="'+ data[dataList]["url"] +'" class="img"><img  src="/assets/img/product_placeholder.png"  data-original="'+ data[dataList]["icon"] +'/350x350" class="img-lazyload img-responsive center-block falsh-img-margin img-main" alt="'+data[dataList]["product_name"]+'"/></a>';
                    arrHtml += '<div class="caption">';
                    arrHtml += '<h2 class="heading"><a href="'+ data[dataList]["url"] +'" class="link-black"`>'+ data[dataList]["product_name"] +'</a></h2>';
                    arrHtml += '<p class="price product-list-price"><s>'+ formatPrice(msrp) +'</s>&nbsp;&nbsp;'+ formatPrice(event_price) +'</p>';
                    if(data[dataList]["event_id"] > 0 ){
                        arrHtml += '<span class="acting">SALE</span>';
                    }
                    if(data[dataList]["is_multiple"]){
                        arrHtml += ' <span class="acting-color">color</span>';
                    }
                    arrHtml += '</div><div class="collect" data-id="'+ data[dataList]["product_id"] +'" data-event_id="0"></div></article>';
                    var data_size = data[dataList]["size_option_value"];
                    if( data_size.length != 0 ){
                        arrHtml += '<p class="caption-size"><i>Size:</i>&nbsp;';
                        for( sizes in data_size){
                            arrHtml += '<a href="' + data_size[sizes] + '">' + sizes + '</a>'
                        }
                        arrHtml += '</p>';
                    }
                    arrHtml += '</div>';
                    dovBox.append(arrHtml);
                }
                $(".img-responsive,.img-lazyload").lazyload({ threshold : 800 ,failure_limit :8, placeholder: '/assets/img/product_placeholder.png', effect: "fadeIn", skip_invisible:false});
                productNumber = $(".category_listview_baby:visible .category-list-item");
                productHeightArray = [];
                productNumber.each(function(){
                    var _this = $(this);
                    setTimeout(function(){
                        productHeightArray.push(_this.offset().top +productHeight);
                    },100);
                });
                scrollTopArray = [];
            },
            error:function(){
                $(".loading_img").hide();
                dovBox.next(".loading_img").addClass("loading_img-no");
            }
        });
    }

