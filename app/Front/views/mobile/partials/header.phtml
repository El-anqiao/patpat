                var hot_search_words = [{"words":"mom and me"},{"words":"Yoga Pants"},{"words":"Dresses"},{"words":"Mermaid swimsuit"},{"words":"daddy and me"},{"words":"Diaper"},{"words":"Swimsuit"},{"words":"Jumpsuit"}]
